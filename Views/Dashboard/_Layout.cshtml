@{
    ViewBag.IsLoggedOut = true;

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Portal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/UserManagement_CodeWithSL.styles.css" asp-append-version="true" />
</head>
<body>

    <partial name="_NavbarPartial" />

    <div class="drawer_panel">
        <div class="side_drawer">
            <p> <a asp-action="Overview" asp-controller="Dashboard">Overview</a></p>
            <p><a asp-action ="ManageUser" asp-controller="Dashboard"></a>Manage User</p>
            <p><a asp-action="ManageRoles" asp-controller="Dashboard">Manage Roles</a></p>

        </div>

        <div class="drawer_content_area">
            <div class="drawer_content_area-inner">
                This is the main area
                @RenderBody()
            </div>
        </div>
    </div>


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - UserManagement_CodeWithSL - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
